#!/usr/bin/env php
<?php

declare(strict_types=1);

require __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use GeoLibre\Command\DatabaseMigrateCommand;

$application = new Application('GeoLibre Console', '1.0.0');

// Register commands
$application->add(new DatabaseMigrateCommand());

$application->run(); 