{
    "name": "geolibre/gis-catalog",
    "description": "GIS data catalog with ISO 19115 metadata and OAI-PMH support",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-pgsql": "*",
        "doctrine/dbal": "^3.6",
        "guzzlehttp/guzzle": "^7.7",
        "monolog/monolog": "^3.3",
        "php-di/php-di": "^7.0",
        "slim/csrf": "^1.5",
        "slim/flash": "^0.4.0",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.11",
        "slim/twig-view": "^3.4",
        "symfony/console": "^6.2",
        "twig/twig": "^3.6",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "GeoLibre\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeoLibre\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
